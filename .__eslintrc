{
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["react", "import"],
    "rules": {
      "react/prop-types": 0,
      "indent": ["error", 2],
      "quotes": ["error", "double"]
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }